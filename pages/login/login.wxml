<view class="logo_container">
    <image class="logo" src="../../img/logo.png" />
</view>
<view class="btn_container">
  <button class="login">手机号登陆</button>
  <button class="experience">立即体验</button>
</view>
<view class="agree">
  <label>
    <view bindtap="toggle" wx:if="{{flag}}"><image src="/img/select.png"></image></view>
    <view bindtap="toggle" wx:else><image src="/img/selected.png"></image></view>
  </label>
  <text>同意<text class="t">《服务条款》</text>和<text class="t">《隐私政策》</text></text>
</view>
<view class="icon">
  <image bindtap="toast" src="../../img/login1.png"></image>
  <image bindtap="toast" src="../../img/login2.png"></image>
  <image bindtap="toast" src="../../img/login3.png"></image>
  <image bindtap="toast" src="../../img/login4.png"></image>
</view>
