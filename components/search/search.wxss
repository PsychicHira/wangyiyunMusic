.searchContainer{
    /* width: 100%;
    height: 140rpx;
     */
     display: flex;
     justify-content: space-around; 
}
.searchInput{
    /* width: 100%; */
    width: 450rpx;
    height: 70rpx;
    background-color:#f7f7f7;
    border-radius: 70rpx;
}
.searchContainer image{
    width: 70rpx;
    height: 70rpx;
}
.searchContent{
    position: absolute;
    left: 50%;
    transform: translateX(-50%);
    height: 70rpx;
    top:0;
    display: flex;
}
.searchContent icon{
    /* margin-top: 37%; */
    /* display: block; */
    /* top:50% */
    /* line-height: 140rpx; */
    position: relative;
    width: 40rpx;
    height: 40rpx;
    top:50%;
    transform: translateY(-50%);
}
.searchContent text{
    line-height: 70rpx;
    margin-left: 10rpx;
    font-size: 30rpx
}